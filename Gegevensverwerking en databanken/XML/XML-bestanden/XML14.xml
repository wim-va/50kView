<?xml version="1.0" ?> 

<!-- File Name: XML14.xml -->

<!DOCTYPE film
	[
	<!ELEMENT film (titel, (ster | regisseur | producent))>
	<!ATTLIST film	
		Klasse (fictie | documentaire | opleiding) "fictie">
	<!ELEMENT titel (#PCDATA)>
	<!ELEMENT ster (#PCDATA)>
	<!ELEMENT regisseur (#PCDATA)>
	<!ELEMENT producent (#PCDATA)>
	]
>

<film>
<!-- attribuut Klasse wordt met defaultwaarde "fictie" toegevoegd aan film -->
	<titel>XML for Dummies</titel>
	<producent>Ingrid Lammers</producent>
</film>
